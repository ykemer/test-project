{
  "name": "spinada",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "node build/server.js",
    "build": "tsc && tsc-alias",
    "dev": "ts-node-dev --require ./tsconfig-paths-bootstrap.js --poll src/server.ts",
    "migrage": "npx sequelize-cli db:migrate",
    "seed-db": "npx sequelize-cli db:seed:all",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "test:acceptance:run": "jest --config=tests/acceptance.jest.config.js --coverage --runInBand",
    "test:unit:run": "jest --config=tests/unit.jest.config.js --coverage --runInBand",
    "test:integration:run": "jest --config=tests/integration.jest.config.js --runInBand",
    "test:e2e:run": "jest --config=tests/e2e.jest.config.js --runInBand",
    "test": " npm run test:integration:run && npm run test:unit:run && npm run test:acceptance:run"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
